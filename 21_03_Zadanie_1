#include <iostream>

using namespace std;

int main()
{   int i;
    cout << "Введите число: \n";
    cin >> i; 
   char c [9][7]= {
       "One",
       "Two",
       "Three",
       "Four",
       "Five",
       "Six",
       "Seven",
       "Eight",
       "Nine"
   };
   
   cout << c[i-1]<<"\n";
   
    return 0;
}
